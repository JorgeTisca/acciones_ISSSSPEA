}

model asentamientos {
  idAsentamiento     Int        @id @unique(map: "idAsentamiento_UNIQUE") @default(autoincrement()) @db.UnsignedInt
  codigopostal       Int
  nombreasentamiento String     @db.VarChar(128)
  tipoasentamiento   String     @db.VarChar(64)
