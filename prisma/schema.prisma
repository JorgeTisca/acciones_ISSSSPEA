model tbl_cuenta_bancaria_solicitud {
  id                   Int       @id @default(autoincrement())
  afiliacion           String    @db.VarChar(30)
  identificacion_1_url String    @db.VarChar(200)
  identificacion_2_url String    @db.VarChar(200)
